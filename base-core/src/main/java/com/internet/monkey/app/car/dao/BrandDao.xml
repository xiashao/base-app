<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.internet.monkey.app.car.dao.BrandDao">

    <select id="getBrandList" resultType="com.internet.monkey.app.car.pojo.Brand">
        SELECT * FROM brand
    </select>
    <select id="getBrandByName" resultType="com.internet.monkey.app.car.pojo.Brand">
        SELECT * FROM brand WHERE name=#{name}
    </select>
</mapper>