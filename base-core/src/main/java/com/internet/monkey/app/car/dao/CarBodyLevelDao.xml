<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.internet.monkey.app.car.dao.CarBodyLevelDao">


    <select id="getCarBodyLevelByName" resultType="com.internet.monkey.app.car.pojo.CarBodyLevel">
        SELECT * FROM car_body_level WHERE level=#{name}
    </select>

    <select id="getAllCarBodyLevel" resultType="com.internet.monkey.app.car.pojo.CarBodyLevel">
        SELECT * FROM car_body_level
    </select>
</mapper>